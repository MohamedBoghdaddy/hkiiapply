{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/$1"
    }
  ],
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/build/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "services/app.py",
      "use": "@vercel/python"
    }
  ]
}
